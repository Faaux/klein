(a0 e0123 + a1 e01 + a2 e02 + a3 e03) ^ (b0 e0123 + b1 e01 + b2 e02 + b3 e03)

# p2 ^ p3
(a0 e0123 + a1 e01 + a2 e02 + a3 e03) ^ (b0 e123 + b1 e021 + b2 e013 + b3 e032)

